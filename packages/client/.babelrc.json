{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": [
              "last 4 iOS versions",
              "last 3 Safari versions",
              "last 1 ChromeAndroid version",
              "last 2 Chrome versions",
              "last 2 Samsung versions",
              "last 2 Firefox versions",
              "last 1 Edge version"
            ]
          }
        }
      }
    ]
  ],
  "plugins": [
    "import-graphql",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ]
  ]
}
